@model IEnumerable<DataModelLibrary.SubscriberStandardSections>

@{
    ViewBag.Title = "Index";
}

<div class="panel panel-primary">
    <div class="panel-heading">
        <h3 class="panel-title">Class Section List</h3>
    </div>
    <div class="panel-body">
        <div id="ContactList">
            <p>
                @Html.ActionLink("Create New", "Create")
            </p>
            <table class="table table-bordered table-hover">
                <tr>
                    <th class="text-primary">
                        @Html.DisplayName("Standard Name")
                    </th>
                    <th class="text-primary">
                        @Html.DisplayName("Section Name")
                    </th>
                    <th class="text-primary">
                        @Html.DisplayNameFor(model => model.Active)
                    </th>
                    <th class="text-primary">
                        Action
                    </th>
                </tr>

                @foreach (var item in Model)
                {
                    <tr>
                        <td>
                            @Html.DisplayFor(modelItem => item.SubscriberStandards.Standard.Name)
                        </td>
                        <td>
                            @Html.DisplayFor(modelItem => item.SubscriberSection.Sections.Name)
                        </td>
                        <td>
                            @Html.DisplayFor(modelItem => item.Active)
                        </td>
                        <td>
                            @Html.ActionLink("Edit", "Edit", new { id = item.Id }) |
                            @Html.ActionLink("Details", "Details", new { id = item.Id }) |
                            @Html.ActionLink("Delete", "Delete", new { id = item.Id })
                        </td>
                    </tr>
                }

            </table>
        </div>
    </div>
</div>

